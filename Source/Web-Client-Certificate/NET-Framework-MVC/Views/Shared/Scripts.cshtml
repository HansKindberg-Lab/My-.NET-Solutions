<script>
	function clearCachedClientCertificate() {
		var clearByClosingBrowserMessage = "To clear the cached client-certificate you have to close your browser.";
		if (window.crypto) {
			try {
				window.crypto.logout();
				alert("The cached client-certificate is removed.");
			}
			catch (exception) {
				alert("window.crypto.logout() does not work. " + clearByClosingBrowserMessage + " Error: " + exception.message);
			}
		}
		else {
			alert("window.crypto does not exist. " + clearByClosingBrowserMessage);
		}
	}
</script>